{
    "name": "@fluxmodels/react",
    "version": "0.0.8",
    "license": "MIT",
    "description": "FluxModels is a type-safe state management library for React applications",
    "author": {
        "name": "Vadzim Sharai",
        "email": "vadzimsharai@gmail.com"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/fluxmodels/fluxmodels.git"
    },
    "bugs": {
        "url": "https://github.com/fluxmodels/fluxmodels/issues"
    },
    "homepage": "https://fluxmodels.dev",
    "tags": [
        "react",
        "model",
        "reactive",
        "meta",
        "state",
        "useState",
        "state management",
        "flux",
        "typescript"
    ],
    "keywords": [
        "react",
        "model",
        "reactive",
        "meta",
        "state",
        "useState",
        "state management",
        "flux",
        "typescript"
    ],
    "main": "./lib/index.js",
    "module": "./lib/index.mjs",
    "types": "./index.d.ts",
    "browser": "./lib/metatyper.min.js",
    "exports": {
        ".": {
            "types": "./index.d.ts",
            "require": "./lib/index.js",
            "import": "./lib/index.mjs"
        }
    },
    "files": [
        "/lib",
        "/index.d.ts"
    ],
    "scripts": {
        "sandbox": "jest ./test/sandbox.test.ts",
        "build": "rimraf ./lib && rollup -c",
        "build:dev": "rimraf ./lib && rollup -cw"
    },
    "peerDependencies": {
        "@types/react": ">=17.0.0",
        "react": ">=17.0.0"
    },
    "dependencies": {
        "@fluxmodels/core": "=0.0.8"
    },
    "devDependencies": {
        "@rollup/plugin-node-resolve": "^15.2.3",
        "@rollup/plugin-terser": "^0.4.4",
        "@rollup/plugin-typescript": "^11.1.6",
        "@testing-library/dom": "^10.3.1",
        "@testing-library/jest-dom": "^6.4.6",
        "@testing-library/react": "^16.0.0",
        "@types/jest": "^29.5.12",
        "@types/node": "^20.14.10",
        "@types/react": "^18.3.3",
        "@types/react-dom": "^18.3.0",
        "globals": "^15.8.0",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "rimraf": "^6.0.1",
        "rollup": "^4.18.1",
        "rollup-plugin-filesize": "^10.0.0",
        "ts-jest": "^29.2.2",
        "typescript": "=5.5.3"
    },
    "packageManager": "yarn@4.4.1",
    "sideEffects": false
}
